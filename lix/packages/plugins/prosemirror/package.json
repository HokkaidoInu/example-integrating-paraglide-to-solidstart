{
	"name": "@lix-js/plugin-prosemirror",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.0.1",
	"license": "Apache-2.0",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"build": "rolldown ./src/index.ts -o ./dist/index.js",
		"test": "tsc --noEmit && vitest run",
		"test:watch": "vitest",
		"lint": "eslint src/**/* --fix",
		"dev": "tsc --watch",
		"format": "prettier ./src --write",
		"ui:dev": "cd ui && pnpm dev"
	},
	"_comment": "Required for tree-shaking https://webpack.js.org/guides/tree-shaking/#mark-the-file-as-side-effect-free",
	"sideEffects": false,
	"dependencies": {
		"@lix-js/sdk": "workspace:*"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@opral/tsconfig": "workspace:*",
		"@vitest/coverage-v8": "^2.0.5",
		"eslint": "^9.12.0",
		"prettier": "^3.3.3",
		"rolldown": "1.0.0-beta.1",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.9.0",
		"vitest": "^2.0.5"
	}
}