{
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"target": "ES2020",
		"experimentalDecorators": true,
		"module": "Node16",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"skipLibCheck": true,
		"moduleResolution": "Node16",
		"moduleDetection": "force",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"checkJs": true,
		"strict": true,
		"noImplicitAny": false
	}
}
