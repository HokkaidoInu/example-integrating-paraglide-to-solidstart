{
  "$schema": "https://app.flightcontrol.dev/schema.json",
  "environments": [
    {
      "id": "production",
      "name": "Production",
      "region": "eu-west-3",
      "source": { "branch": "main", "pr": false, "trigger": "push" },
      "services": [
        {
          "id": "opral-website-7nvwjx",
          "name": "Opral Website",
          "buildType": "nixpacks",
          "watchPaths": ["packages/opral-website/**"],
          "type": "static",
          "ci": {
            "type": "ec2",
            "instanceStorage": 30,
            "storageThroughput": 125,
            "storageIops": 3000,
            "storageType": "gp3"
          },
          "envVariables": {},
          "basePath": ".",
          "versionHistoryCount": 10,
          "outputDirectory": "packages/opral-website",
          "singlePageApp": false,
          "cloudfrontAutoCacheInvalidation": true,
          "originShieldEnabled": true
        }
      ]
    }
  ]
}
