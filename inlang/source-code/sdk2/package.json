{
	"name": "@inlang/sdk2",
	"version": "2.0.0-prerelease.0",
	"type": "module",
	"license": "Apache-2.0",
	"homepage": "https://inlang.com/documentation/sdk",
	"repository": {
		"type": "git",
		"url": "https://github.com/opral/inlang-sdk"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
