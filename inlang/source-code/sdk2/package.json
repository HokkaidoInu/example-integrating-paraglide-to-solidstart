		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./node_modules"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"bundleDependencies": [
		"@lix-js/sdk",
		"sqlite-wasm-kysely"
	],
	"dependencies": {
		"@lix-js/sdk": "workspace:*",
		"@sinclair/typebox": "^0.31.17",
		"dedent": "1.5.1",
		"kysely": "^0.27.4",
		"murmurhash3js": "^3.0.1",
		"rxjs": "7.8.1",
		"sqlite-wasm-kysely": "workspace:*",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@types/murmurhash3js": "^3.0.7",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-v8": "^2.0.5",
		"memfs": "4.6.0",
		"typescript": "^5.5.2",
